{
  "name": "@appx/example-plugin",
  "version": "0.0.0",
  "description": "Appx example plugin",
  "author": "Water.li",
  "license": "MIT",
  "repository": "rxdrag/plugins/example",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest"
  },
  "jest": {
