    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@formily/react": "^2.2.5",
    "@formily/reactive-react": "^2.2.5",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.7",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^16.13.1",
    "rollup": "^1.29.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.1.3",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react hooks",
    "typescript",
    "npm"
  ],
  "dependencies": {
    "@designable/core": "^1.0.0-beta.45",
    "@designable/react": "^1.0.0-beta.45",
    "@formily/json-schema": "^2.2.5",
    "prop-types": "^15.7.2"
  }
